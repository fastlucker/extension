{
  "name": "ambire-mobile-wallet",
  "main": "index.js",
  "scripts": {
    "android": "EAS_BUILD_PLATFORM=android bash ./scripts/expo-build-post-install.sh && expo run:android",
    "android:staging": "APP_ENV=staging yarn android",
    "android:production": "APP_ENV=production yarn android",
    "ios": "EAS_BUILD_PLATFORM=ios bash ./scripts/expo-build-post-install.sh && expo run:ios",
    "ios:staging": "APP_ENV=staging yarn ios",
    "ios:production": "APP_ENV=production yarn ios",
    "web:webkit": "rm -rf ./build/webkit-dev && WEB_ENGINE=webkit WEBPACK_BUILD_OUTPUT_PATH=webkit-dev APP_ENV=development expo start --web",
    "web:safari": "./scripts/safari-dev-build.sh",
    "web:gecko": "rm -rf ./build/gecko-dev && WEB_ENGINE=gecko WEBPACK_BUILD_OUTPUT_PATH=gecko-dev APP_ENV=development expo start --web",
    "web:benzin": "rm -rf ./build/benzin-dev && WEBPACK_BUILD_OUTPUT_PATH=benzin-dev APP_ENV=development RCT_METRO_PORT=3006 WEB_PORT=3000 expo start --web",
    "web:legends": "rm -rf ./build/legends-dev && WEBPACK_BUILD_OUTPUT_PATH=legends-dev APP_ENV=development RCT_METRO_PORT=3006 WEB_PORT=3000 expo start --web",
    "start": "react-native start",
    "start:clean": "react-native start --reset-cache",
    "start:staging": "APP_ENV=staging yarn start",
    "start:clean:staging": "APP_ENV=staging yarn start:clean",
    "start:production": "APP_ENV=production yarn start",
    "start:clean:production": "APP_ENV=production yarn start:clean",
    "eas-build-post-install": "./scripts/expo-build-post-install.sh",
    "build:configure": "eas build:configure",
    "build:android:development": "eas build -p android --profile development",
    "build:android:staging": "eas build -p android --profile staging",
    "build:android:production": "eas build -p android --profile production",
    "build:ios:development": "eas build -p ios --profile development",
    "build:ios:staging": "eas build -p ios --profile staging",
    "build:ios:production": "eas build -p ios --profile production",
    "build:ios:production:simulator": "eas build -p ios --profile production:simulator",
    "build:android:production:simulator": "eas build -p android --profile production:simulator",
    "build:web:webkit": "rm -rf ./build/webkit-prod && APP_ENV=production WEB_ENGINE=webkit WEBPACK_BUILD_OUTPUT_PATH=webkit-prod expo export:web",
    "build:web:safari": "./scripts/safari-prod-build.sh",
    "build:web:gecko": "rm -rf ./build/gecko-prod && APP_ENV=production WEB_ENGINE=gecko WEBPACK_BUILD_OUTPUT_PATH=gecko-prod expo export:web",
    "build:web:benzin": "rm -rf ./build/benzin-prod && APP_ENV=production WEBPACK_BUILD_OUTPUT_PATH=benzin-prod expo export:web",
    "build:web:legends": "rm -rf ./build/legends-prod && APP_ENV=production WEBPACK_BUILD_OUTPUT_PATH=legends-prod expo export:web",
    "publish:development": "expo publish --release-channel development",
    "publish:staging": "APP_ENV=staging expo publish --release-channel staging",
    "publish:production": "APP_ENV=production expo publish --release-channel default",
    "translations:scan": "i18next-scanner --config ./scripts/i18next-scanner.config.js './src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint ./src/** --ext .js,.jsx,.ts,.tsx --fix",
    "expo:doctor": "expo doctor",
    "expo:upgrade": "expo upgrade",
    "expo:install": "expo install",
    "eas:secret:list": "eas secret:list",
    "eas:secret:push": "eas secret:push --scope project --env-file .env",
    "test:e2e:web:dev": "WEBPACK_BUILD_OUTPUT_PATH=webkit-dev jest --config=./tests/jest.config.js -- ./tests",
    "test:e2e:web:dev:local": "docker stop $(docker ps -a -q) && docker-compose -f ./docker-compose-mac.yml up --force-recreate -d && WEBPACK_BUILD_OUTPUT_PATH=webkit-dev jest --config=./tests/jest.config.js -- ./tests",
    "test:e2e:web": "yarn build:web:webkit && jest test --config=./tests/jest.config.js",
    "test:e2e:web:local": "docker stop $(docker ps -a -q) && docker-compose -f ./docker-compose-mac.yml up --force-recreate -d && yarn build:web:webkit && jest test --config=./tests/jest.config.js",
    "test": "jest --config=./jest.config.js",
    "postinstall": "patch-package"
  },
